<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="acneThreeVM" type="com.heddymvp.app.modules.acnethree.data.viewmodel.AcneThreeVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAcneThree"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnheddy"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_11pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtHeddy"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{acneThreeVM.acneThreeModel.txtHeddy}"
                style="@style/txtFredokasemibold30" tools:text="@string/lbl_heddy"/>
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_width="@dimen/_6pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                <View
                    android:background="@drawable/rectangle_bg_yellow_700_radius_5"
                    android:id="@+id/viewRectangle973"
                    android:layout_height="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_27pxh" android:layout_width="@dimen/_321pxh"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWhereisyoura"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{acneThreeVM.acneThreeModel.txtWhereisyoura}"
                style="@style/txtIntersemibold24" tools:text="@string/msg_where_is_your_a"/>
            <TextView android:id="@+id/txtBasedonthein"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:text="@{acneThreeVM.acneThreeModel.txtBasedonthein}"
                style="@style/txtRobotoromanregular17_1" tools:text="@string/msg_based_on_the_in"/>
            <androidx.appcompat.widget.LinearLayoutCompat
                android:id="@+id/chooseLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnFace"
                android:layout_width="180dp"
                android:layout_height="33dp"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_face"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_face"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBack"
                android:layout_width="180dp"
                android:layout_height="33dp"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_back"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_back"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnNeck"
                android:layout_width="180dp"
                android:layout_height="33dp"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_neck"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_neck"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnOther"
                android:layout_width="180dp"
                android:layout_height="33dp"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_6pxv"
                android:singleLine="true"
                android:text="@string/lbl_other2"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_other2"/>

            </androidx.appcompat.widget.LinearLayoutCompat>
            <LinearLayout android:id="@+id/linearRowframe389"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_200pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFrame389"
                    android:layout_height="@dimen/_62pxh"
                    android:layout_width="@dimen/_62pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame389"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame389"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_62pxh"
                    android:layout_marginStart="@dimen/_213pxh"
                    android:layout_width="@dimen/_62pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_blue_a400_62x62"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_blue_a400_62x62"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
