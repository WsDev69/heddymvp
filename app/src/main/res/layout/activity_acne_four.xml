<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="acneFourVM" type="com.heddymvp.app.modules.acnefour.data.viewmodel.AcneFourVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAcneFour"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImageFive"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_image5_797x390}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image5_797x390"/>
        <LinearLayout android:id="@+id/linearColumnheddy"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_11pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtHeddy"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{acneFourVM.acneFourModel.txtHeddy}"
                style="@style/txtFredokasemibold30" tools:text="@string/lbl_heddy"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndifferentkinds"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_36pxh"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginTop="@dimen/_114pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_101">
                <TextView android:gravity="start"
                    android:id="@+id/txtDifferentkinds"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_269pxv"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_34pxv"
                    android:layout_width="match_parent"
                    android:text="@{acneFourVM.acneFourModel.txtDifferentkinds}"
                    style="@style/txtRobotoromanmedium16_1" tools:text="@string/msg_different_kinds"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageArrowright"
                android:layout_gravity="end"
                android:layout_height="@dimen/_62pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_163pxv"
                android:layout_width="@dimen/_62pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright_blue_a400_62x62"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_blue_a400_62x62"/>
        </LinearLayout>
    </FrameLayout>
</layout>
