<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="acneSevenVM" type="com.heddymvp.app.modules.acneseven.data.viewmodel.AcneSevenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameAcneSeven"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnheddy"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_11pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:id="@+id/txtHeddy"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{acneSevenVM.acneSevenModel.txtHeddy}"
                style="@style/txtFredokasemibold30" tools:text="@string/lbl_heddy"/>
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_width="@dimen/_6pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_black_900"/>
                <View
                    android:background="@drawable/rectangle_bg_yellow_700_radius_5"
                    android:id="@+id/viewRectangle973"
                    android:layout_height="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_27pxh" android:layout_width="@dimen/_321pxh"/>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_110pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:text="@{acneSevenVM.acneSevenModel.txtDescription}"
                style="@style/txtIntermedium24" tools:text="@string/msg_based_on_your_a"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnYesTakeMeThereOne"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_68pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="@dimen/_281pxh"
                android:paddingBottom="@dimen/_22pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_22pxv"
                android:singleLine="true"
                android:textSize="@dimen/_19sp"
                android:text="@string/msg_yes_take_me_th"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/msg_yes_take_me_th"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnnoiwouldpre"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStylegray_1">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtNoIwouldpre"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginEnd="@dimen/_50pxh"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:text="@{acneSevenVM.acneSevenModel.txtNoIwouldpre}"
                    style="@style/txtRobotoromanmedium15" tools:text="@string/msg_no_i_would_pre"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
